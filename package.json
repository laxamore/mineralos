{
  "name": "mineralos",
  "version": "1.0.0",
  "scripts": {
    "api:dev": "reflex -r '\\.go$' -s -- sh -c 'go run main.go'",
    "api:start": "go run main.go",
    "api:build": "go build -o bin/",
    "server:dev": "reflex -r '\\.go$' -s -- sh -c 'go run server/main.go'",
    "server:start": "go run server/main.go",
    "server:build": "go build -o bin/server server/main.go"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laxamore/mineralos.git"
  },
